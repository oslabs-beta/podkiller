{
  "type": "module",
  "name": "podkiller-cli",
  "version": "1.0.0",
  "description": "We kill k8 pods",
  "main": "index.js",
  "bin": {
    "podkiller": "./bin/commander.js"
  },
  "scripts": {
    "start": "minikube start && npm run commander",
    "stop": "minikube stop",
    "commander": "NODE_NO_WARNINGS=1 node bin/commander.js",
    "dashboard": "nodemon --watch src/server.js --ignore reports/ src/server.js & xdg-open http://localhost:3000"
  },
  "keywords": [
    "chaos",
    "kubernetes",
    "cli",
    "devops"
  ],
  "author": "Team Chaos Panda",
  "license": "MIT",
  "dependencies": {
    "@kubernetes/client-node": "^1.3.0",
    "axios": "^1.11.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "inquirer": "^12.9.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
